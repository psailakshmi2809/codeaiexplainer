{
  "name": "ezpay-api-svc",
  "version": "0.0.1",
  "description": "",
  "main": "",
  "scripts": {
    "test": "pwsh ./azp-yaml/scripts/ezpay-api-svc-unit-tests.ps1 -workingDirectory .",
    "test:ci": "jest --ci --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura",
    "test:ci-skip-integration": "jest --ci --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --testPathIgnorePatterns='/integration/'",
    "test:watch": "jest --coverage --watchAll",
    "build": "pwsh ./azp-yaml/scripts/ezpay-api-svc-compile.ps1 -workingDirectory . -outputDirectory ./dist",
    "build:tsc": "tsc",
    "build:docker": "pwsh ./azp-yaml/scripts/ezpay-api-svc-docker.ps1 -workingDirectory . -dockerFilePath ./Dockerfile",
    "build:watch": "onchange 'src/**/*.ts' -- yarn build",
    "view:coverage": "serve ./coverage/lcov-report",
    "clean": "rimraf ./dist && rimraf ./coverage && rimraf ./node_modules",
    "clean:dist": "rimraf ./dist",
    "lint": "node --max_old_space_size=4096 ./node_modules/eslint/bin/eslint.js . --cache --ext js,jsx,ts,tsx,md",
    "prettier:check": "prettier --check '**/*.ts'",
    "prettier:write": "prettier --write '**/*.ts'",
    "serve": "nodemon ./dist/index.js 4000",
    "serve:debug": "nodemon --inspect=9230 ./dist/index.js 4000",
    "start": "yarn clean:dist && yarn build && yarn serve",
    "start:debug": "yarn clean:dist && yarn build && yarn serve:debug",
   
"grpc:gen-ts-win": "mkdirp ./src/code-gen/proto && grpc_tools_node_protoc --ts_out=./src/code-gen/proto --plugin=protoc-gen-ts=%CD%/node_modules/.bin/protoc-gen-ts.cmd -I ../../protos ../../protos/*.proto",
"grpc:gen-js": "mkdirp ./src/code-gen/proto && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/code-gen/proto --grpc_out=grpc_js:./src/code-gen/proto -I ../../protos ../../protos/*.proto",
"grpc:gen-ts": "mkdirp ./src/code-gen/proto && grpc_tools_node_protoc --ts_out=./src/code-gen/proto --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts -I ../../protos ../../protos/*.proto",
"grpc:gen-js-win": "mkdirp ./src/code-gen/proto && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/code-gen/proto --grpc_out=grpc_js:./src/code-gen/proto -I ../../protos ../../protos/*.proto",
"grpc:gen-js-docker": "mkdirp ./src/code-gen/proto && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/code-gen/proto --grpc_out=grpc_js:./src/code-gen/proto -I ./protos ./protos/*.proto",
"grpc:gen-ts-docker": "mkdirp ./src/code-gen/proto && grpc_tools_node_protoc --ts_out=./src/code-gen/proto --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts -I ./protos ./protos/*.proto",

    "data:create-new-account-for-onboarding-developer": "node ./src/scripts/createNewAccountForOnboardingDeveloper.js",
    "data:create-new-account-for-onboarding-product": "node ./src/scripts/createNewAccountForOnboardingProduct.js",
    "data:create-payment-requests-for-search": "node ./src/scripts/createSamplePaymentRequestSearchDocs.js",
    "data:publish-platform-events": "node ./src/scripts/publishPlatformEvents.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
